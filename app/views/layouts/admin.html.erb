<!DOCTYPE html>
<html>
<head>
  <title><%= meta_title %></title>
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- <meta name="viewport" content="width=device-width, user-scalable=no"> -->
  <%= stylesheet_link_tag    'admin/admin', media: 'all', 'data-turbolinks-track' => true %>
  <%= yield :extra_css %>
  <%= javascript_include_tag 'admin/admin', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%#= favicon_link_tag "favicon.ico" %>
</head>
<body class="admin <%= controller_name %> <%= action_name %>">
  <%= notice_message %>
  
  <%= yield %>
  <%= yield :custom_js %>

  <script type="text/javascript">
    $(".btn-tech").on('mouseover', function(){
      $("#genericHover")[0].play();
    })
  </script>
</body>
</html>
