<p id="notice"><%= notice %></p>
<% console %>
<h1>Listing Links</h1>
<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Fetch time</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |link| %>
      <tr>
        <td><%= link_to "連結", link.value %></td>
        <td><%= link.fetch_time %></td>
        <td><%= link.state %></td>
        <td><%= link_to 'Show', link %></td>
        <td><%= link_to 'Edit', edit_link_path(link) %></td>
        <td><%= link_to 'Destroy', link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Link', new_link_path %>
